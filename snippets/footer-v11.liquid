{% if section.settings.select_fts == "footer_v11" %}
{% if template contains 'index' %}
<footer class="footer_v11" style="background-color : {{section.settings.ft_home_bg_footer}}">
  <div class="top-footer">
    <div class="container container-v4">    
      <div class="row">
        <div class="col-md-6">
          <div class="footer-content">
            <div class="title_footer ">          
              <h4 class="mb-0 title_border">{{settings.title_menu_fv11_1}}</h4>         
            </div>
            <div class="row">
              <div class="col-md-6">
                <div class="menu_footer">
                  <ul class="list-unstyled mb-0">
                    {% for link in linklists[settings.menu_fv11_1].links %}
                    <li class=" mr-0"><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
                    {% endfor %}
                  </ul>        
                </div>
              </div>
              <div class="col-sm-6">
                <div class="menu_footer">
                  <ul class=" list-unstyled mb-0">
                    {% for link in linklists[settings.menu_fv11_2].links %}
                    <li class=" mr-0"><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
                    {% endfor %}
                  </ul>        
                </div>

              </div>
            </div>
          </div>
        </div>
        
        <div class="col-md-6">
          <div class="info_footer end">
            <div class="title_footer ">          
              <h4 class="mb-0 title_border">{{settings.title_menu_fv11_2}}</h4>         
            </div>
            {% if settings.content_fv11 != blank  %}
            <p class="content_footer">{{settings.content_fv11}}</p>
            {% endif %}

            {% if settings.newsletter_form_action != blank %}
            {% assign form_action = settings.newsletter_form_action %}
            {% else %}
            {% assign form_action = '#' %}
            {% endif %}   
            <div class="newsletter">
              <form class="needs-validation fv4-form " action="{{form_action}}" method="post">
                <div class="mb-0">                           
                  <input type="email" name="EMAIL" class=" text-input" placeholder="{{ 'general.newsletter_form.newsletter_email' | t }}">                 
                </div>
                <button class="btn" type="submit">
                  <svg id="_005-email" data-name="005-email" xmlns="http://www.w3.org/2000/svg" width="21" height="15" viewBox="0 0 21 15">
                    <g id="Group_877" data-name="Group 877">
                      <path id="Path_1349" data-name="Path 1349" d="M20.25,68.267H.75a.75.75,0,0,0-.75.75v13.5a.75.75,0,0,0,.75.75h19.5a.75.75,0,0,0,.75-.75v-13.5A.75.75,0,0,0,20.25,68.267Zm-1.23,1.5L10.5,76.321,1.98,69.767Zm.48,12H1.5V71.29l8.543,6.572a.75.75,0,0,0,.913,0L19.5,71.29Z" transform="translate(0 -68.267)"/>
                    </g>
                  </svg>
                </button>
              </form>      
            </div>
          </div>
        </div>        
      </div>
    </div>
  </div>
  <div class="middle-footer">
    <div class="container container-v4">
      <div class="row">
        <div class="col-md-6">
          <div class="footer-content">
            <div class="title_footer ">          
              <h4 class="mb-0 title_border">{{settings.title_menu_fv11_3}}</h4>         
            </div>
            <div class="row">
              <div class="col-md-6">
                <div class="menu_footer">
                  <ul class="list-unstyled mb-0">
                    {% for link in linklists[settings.menu_fv11_3].links %}
                    <li class=" mr-0"><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
                    {% endfor %}
                  </ul>        
                </div>
              </div>
              <div class="col-sm-6">
                <div class="menu_footer">
                  <ul class=" list-unstyled mb-0">
                    {% for link in linklists[settings.menu_fv11_4].links %}
                    <li class=" mr-0"><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
                    {% endfor %}
                  </ul>        
                </div>

              </div>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="info_footer">
            <div class="title_footer ">          
              <h4 class="mb-0 title_border">{{settings.title_menu_fv11_4}}</h4>    
            </div>
            {% if settings.content_fv11_2 != blank  %}
            <p class="content_footer">{{settings.content_fv11_2}}</p>
            {% endif %}
            <div class="list-icon ">
              <ul class="list-inline list-unstyled mb-0">
                {% if settings.icon_sc_fv11_1 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv11_1}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv11_1}}"></i></a>
                </li>
                {% endif %}
                {% if settings.icon_sc_fv11_2 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv11_2}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv11_2}}"></i></a>
                </li>
                {% endif %}
                {% if settings.icon_sc_fv11_3 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv11_3}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv11_3}}"></i></a>
                </li>
                {% endif %}
                {% if settings.icon_sc_fv11_4 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv11_4}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv11_4}}"></i></a>
                </li>
                {% endif %}         

              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {% if settings.enable_fv11_bottom %}
  <div class="copyright">
    <div class="container container-v4">
      <div class="row align-items-center">
        <div class="col-md-6  {% if settings.enable_rtl_mode %} text-md-right {% else %}text-md-left {% endif %} text-center">
          {% if settings.fv11_bottom_copyright != blank %}
          <p class="text-copyright mb-0">{{settings.fv11_bottom_copyright}}</p>
          {% else %}
          {% if settings.enable_rtl_mode %}
          <div class="text-copyright mb-0"><p>© <a href="https://www.shopify.com/?ref=shopifycenter" rel="nofollow" target="_blank" class=""> Powered by Shopify.</a>  <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover link"> ThemeOcean </a> By  <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover bold">  Foden </a> | 2021 Copyright   </p> 
          </div>          
          {% else %}
          <div class="text-copyright mb-0"><p>© Copyright 2021 | <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover bold">  Foden </a> By <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover link"> ThemeOcean.</a><a href="https://www.shopify.com/?ref=shopifycenter" rel="nofollow" target="_blank" class=""> Powered by Shopify.</a></p> 
          </div>
          {% endif %}
          {% endif %}
        </div>
        
        <div class="col-md-6  {% if settings.enable_rtl_mode %} text-md-left {% else %}text-md-right {% endif %} text-center">
          {% if settings.fv11_bottom_payment_img != blank %}
          <img src="{{settings.fv11_bottom_payment_img | img_url:'master'}}"  alt="">
          {% else %}
          <img src="//via.placeholder.com/247x23"  alt="">
          {% endif %}
        </div>
      </div>
    </div>
  </div>
  {% endif %}

</footer>
<style>
  .footer_v11 .title_footer h4{
    color : {{section.settings.ft_home_color_heading}};
  }
  .footer_v11 .title_footer h4:before{
    background-color : {{section.settings.ft_home_color_heading}};
  }
  .footer_v11 .content_footer{
    color : {{section.settings.ft_home_color_text}};
  }
  
  .footer_v11 .menu_footer ul li{
    color : {{section.settings.ft_home_color_text}};
  }
  .footer_v11 .menu_footer ul li a{
    color : {{section.settings.ft_home_color_text}};
  }
  .footer_v11 .top-footer .info_footer ul li .social-item{
    color : {{section.settings.ft_home_color_text}};
  }
  
</style>
{% endif %}
{% endif %}

