{% if section.settings.select_fts == "footer_v3" %}
{% if template contains 'index' %}
<footer class=" footer_v3">
  <div class="top-footer">
    <div class="container container-v4">    
      <div class="row">
        <div class="col-lg-5 col-md-6">
          <div class="info_footer end">
            <div class="title_footer">          
              <h4 class="mb-0 title_border">{{settings.titlecolum_fv3_5}}</h4>         
            </div>
            <div class="content">
              <div class="phone-icon">
                {% if settings.logo_svg3 !=blank %}
                <div class="icon">{{settings.icon_svg3}}</div>
                {% else %}
                <div class="image">
                  {% if settings.fv3_main_logo != blank %}
                  <img src="{{settings.fv3_main_logo | img_url:'master'}}" class="img-fluid">
                  {% else %}
                  <img src="//via.placeholder.com/81x81" />
                  {% endif %}
                </div>
                {% endif %}
                <div class="txt-left">{{settings.htmlcolum_fv3_1}}</div>
              </div>
              <div class="address">{{settings.htmlcolum_fv3_2}}</div>
            </div>      
            <div class="list-icon">
              <ul class="list-inline list-unstyled mb-0">
                {% if settings.icon_sc_fv3_1 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv3_1}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv3_1}}"></i></a>
                </li>
                {% endif %}
                {% if settings.icon_sc_fv3_2 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv3_2}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv3_2}}"></i></a>
                </li>
                {% endif %}
                {% if settings.icon_sc_fv3_3 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv3_3}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv3_3}}"></i></a>
                </li>
                {% endif %}
                {% if settings.icon_sc_fv3_4 !=blank %}
                <li class="list-inline-item">
                  <a href="{{settings.url_sc_fv3_4}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_fv3_4}}"></i></a>
                </li>
                {% endif %}         

              </ul>
            </div>
          </div>
        </div>
        <div class="col-lg-2 col-md-6">
              <div class="info_footer">
                <div class="title_footer">          
                  <h4 class="mb-0 title_border">{{settings.titlecolum_fv3_1}}</h4>         
                </div>
                <ul class="list-unstyled mb-0">
                  {% for link in linklists[settings.menu_fv3_1].links %}
                  <li><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
                  {% endfor %}
                </ul>        
              </div>
            </div>       
        <div class="col-lg-2 col-md-6">
          <div class="info_footer">
            <div class="title_footer">          
              <h4 class="mb-0 title_border">{{settings.titlecolum_fv3_2}}</h4>         
            </div>
            <ul class="list-unstyled mb-0">
              {% for link in linklists[settings.menu_fv3_2].links %}
              <li><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
              {% endfor %}
            </ul>        
          </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="info_footer">
              <div class="title_footer">          
                <h4 class="mb-0 title_border">{{settings.titlecolum_fv3_3}}</h4>         
              </div>
              <ul class="list-unstyled mb-0">
                {% for link in linklists[settings.menu_fv3_3].links %}
                <li><a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a></li>
                {% endfor %}
              </ul>        
            </div>
          </div>          
      </div>
    </div>
  </div>
  {% if settings.enable_fv3_bottom %}
  <div class="copyright">
    <div class="container container-v4">
      <div class="row align-items-center">
        <div class="col-lg-6 col-md-6  {% if settings.enable_rtl_mode %} text-lg-right text-md-right {% else %}text-lg-left text-md-left{% endif %}">
          {% if settings.fv3_bottom_copyright != blank %}
          <p class="text-copyright">{{settings.fv3_bottom_copyright}}</p>
          {% else %}
          <div class="text-copyright">
            {% if settings.enable_rtl_mode %}
            <p> <a href="https://www.shopify.com/?ref=shopifycenter" rel="nofollow" target="_blank" class=""> Powered by Shopify.</a> <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover link"> ThemeOcean.</a> By <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover bold">  Foden </a>Copyright 2021 |</p> 
            {% else %}
            <p>© Copyright 2021 | <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover bold">  Foden </a> By <a href="http://themeocean.net/" rel="nofollow" target="_blank" class="underline_hover link"> ThemeOcean.</a><a href="https://www.shopify.com/?ref=shopifycenter" rel="nofollow" target="_blank" class=""> Powered by Shopify.</a></p> 
            {% endif %}
          </div> 
          {% endif %}
        </div>
        <div class="col-lg-6 col-md-6 {% if settings.enable_rtl_mode %} text-lg-left text-md-left {% else %}text-lg-right text-md-right {% endif %} ">
          {% if settings.fv3_bottom_payment_img != blank %}
          <img src="{{settings.fv3_bottom_payment_img | img_url:'master'}}"  alt="">
          {% else %}
          <img src="//via.placeholder.com/247x23"  alt="">
          {% endif %}
        </div>
      </div>
    </div>
  </div>
  {% endif %}
</footer>
<style>
  .footer_v3 .top-footer{
    background-color : {{section.settings.ft_home_bg_footer}}
  }
  .footer_v3 .copyright{
    background-color : {{section.settings.ft_home_bg_footer}}
  }
  .footer_v3 .top-footer .title_footer h4{
    color : {{section.settings.ft_home_color_heading}};
  }
  .footer_v3 .top-footer .info_footer ul li a{
    color : {{section.settings.ft_home_color_text}};
  }
  .footer_v3 .top-footer ul li{
    color : {{section.settings.ft_home_color_text}};
  }
  .footer_v3 .top-footer .info_footer.end .list-icon ul li a{
    color : {{section.settings.ft_home_color_text}};
    border-color: {{section.settings.ft_home_color_text}};
  }
  .footer_v3 .top-footer .info_footer.end .content_footer{
    color : {{section.settings.ft_home_color_text}};
  }
  .footer_v3 .copyright .text-copyright{
    color : {{section.settings.ft_home_color_text}};
  }
  .footer_v3 .copyright .text-copyright a{
    color : {{settings.color_main2}};
  }
</style>
{% endif %}
{% endif %}

